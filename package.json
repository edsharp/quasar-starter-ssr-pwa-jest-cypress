{
  "name": "ssr-starter-kit",
  "version": "1.0.0",
  "description": "A Quasar Framework app",
  "productName": "Quasar App",
  "cordovaId": "org.cordova.quasar.app",
  "author": "D.C. Thompson",
  "private": true,
  "scripts": {
    "init:feathers": "yarn && cd server-feathers && yarn",
    "init:graphql": "yarn && cd server-graphql && yarn",
    "lint": "eslint --ext .js,.vue src",
    "lint:fix": "eslint --ext .js,.vue src --fix",
    "db:prisma:init": "cd server-graphql && prisma login && prisma deploy",
    "db:prisma:deploy": "cd server-graphql && prisma deploy",
    "db:graphql:serve": "cd server-graphql && yarn serve:graphql",
    "db:graphql:deploy": "cd server-graphql && yarn deploy:graphql",
    "dev:spa": "NODE_OPTIONS=--max_old_space_size=4096 DEBUG=v8:* quasar dev -m spa",
    "dev:ssr": "NODE_OPTIONS=--max_old_space_size=4096 DEBUG=v8:* quasar dev -m ssr",
    "test:unit": "NODE_ENV=dev jest",
    "test:unit:watch": "NODE_ENV=dev jest --watch",
    "test:e2e": "cypress open",
    "test:e2e:CI": "cypress run",
    "test:lighthouse:ssr": "lighthouse http://localhost:3000 view --output-path=./test/lighthouse/reports/index.html --config-path=./test/lighthouse/configs/full-config.js",
    "build:spa": "quasar build -m ssr",
    "build:ssr": "quasar build -m ssr",
    "serve:spa": "quasar serve ./dist/spa-mat --port 3000",
    "serve:ssr": "nodemon node ./dist/ssr-mat/index.js",
    "deploy:ssr-now": "cd dist/ssr-mat && now",
    "deploy:graphql-now": "cd server-graphql/src && now",
    "deploy:ssr-pm2": "pm2 start ./dist/ssr-mat/index.js --name quasar-ssr",
    "deploy:ssr-pm2_clusterize": "pm2 start ./dist/ssr-mat/index.js --name quasar-ssr -i max",
    "deploy:ssr-pm2_watch": "pm2 start ./dist/ssr-mat/index.js --name quasar-ssr --watch ./dist/ssr-mat",
    "deploy:ssr-pm2_deep-monitoring": "pm2 start ./dist/ssr-mat/index.js --name quasar-ssr --deep-monitoring",
    "deploy:ssr-pm2_restart": "pm2 restart quasar-ssr",
    "deploy:ssr-pm2_stop": "pm2 stop quasar-ssr",
    "deploy:ssr-pm2_monitor": "pm2 monitor quasar-ssr",
    "deploy:ssr-pm2_unmonitor": "pm2 unmonitor quasar-ssr",
    "deploy:ssr-pm2_kill": "pm2 kill",
    "serve:test:coverage": "quasar serve test/coverage/lcov-report/ --port 8788",
    "serve:test:lighthouse": "quasar serve test/lighthouse/reports/ --port 8789",
    "serve:dev:ngrok": "ngrok http 8000 --region eu",
    "serve:dist:ngrok": "ngrok http 3000 --region eu"
  },
  "dependencies": {
    "@babel/core": "^7.0.0-rc.2",
    "@babel/runtime": "^7.0.0-rc.2",
    "@feathersjs/client": "^3.5.4",
    "apollo-cache-inmemory": "^1.2.8",
    "apollo-client": "^2.4.0",
    "apollo-link": "^1.2.2",
    "apollo-link-http": "^1.5.4",
    "apollo-link-watched-mutation": "^0.1.0",
    "axios": "^0.18.0",
    "babel-plugin-graphql-tag": "^1.6.0",
    "es6-iterator": "^2.0.3",
    "feathers-vuex": "^1.4.8",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "socket.io-client": "^2.1.1",
    "vue-apollo": "^3.0.0-beta.19",
    "vue-i18n": "^7.3.3",
    "vuelidate": "^0.7.4",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.0.0-rc.2",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0-rc.2",
    "@babel/plugin-proposal-function-sent": "^7.0.0-rc.2",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0-rc.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-rc.2",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0-rc.2",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-rc.2",
    "@babel/plugin-transform-runtime": "^7.0.0-rc.2",
    "@babel/preset-env": "^7.0.0-rc.2",
    "@babel/preset-stage-2": "^7.0.0-rc.2",
    "@babel/types": "^7.0.0-rc.2",
    "@vue/test-utils": "^1.0.0-beta.23",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^23.4.2",
    "chai": "^4.1.2",
    "cypress": "^3.1.0",
    "eslint": "^4.18.2",
    "eslint-config-standard": "^11.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^4.3.0",
    "eslint-plugin-vue-a11y": "^0.0.26",
    "fse": "^4.0.1",
    "jest": "^23.4.2",
    "jest-serializer-vue": "^2.0.2",
    "jsdom": "^11.12.0",
    "jsdom-global": "^3.0.2",
    "lighthouse": "^3.0.3",
    "node-fetch": "^2.2.0",
    "nodemon": "^1.18.3",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "quasar-cli": "^0.17.13",
    "request-promise": "^4.2.2",
    "strip-ansi": "=3.0.1",
    "vue-jest": "^2.6.0"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.6.0",
    "yarn": ">= 1.6.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
